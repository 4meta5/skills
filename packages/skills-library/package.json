{
  "name": "@anthropic/skills-library",
  "version": "1.0.0",
  "description": "TypeScript library for managing Claude AI skills",
  "type": "module",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    }
  },
  "files": [
    "dist",
    "skills"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:property": "vitest run --config vitest.property.config.ts",
    "test:all": "vitest run && vitest run --config vitest.property.config.ts",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "precommit": "npm run typecheck && npm test"
  },
  "keywords": [
    "claude",
    "ai",
    "skills",
    "anthropic"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "yaml": "^2.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "fast-check": "^4.5.3",
    "lefthook": "^2.0.16",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
